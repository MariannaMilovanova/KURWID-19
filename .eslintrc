{
  "root": true,
  "extends": ["@shelf/eslint-config/frontend-typescript"],
  "settings": {
    "react": {
      "version": "16.9.0"
    }
  },
  "rules": {
    "react/prop-types": "warn",
    "i18next/no-literal-string": [
      "warn",
      {"ignoreCallee": ["t", "createBlock", "logger", "it", "describe"], "ignoreAttribute": ["displayName"]}
    ]
  }
}
